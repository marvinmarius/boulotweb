<script async type='text/javascript'>(function ($, window) {

    let win = $(window);

    // all the sticky elements
    let degreeFinder;

    // the position at which to make all stickies stuck/unstuck
    let stickyPoint;

    // some heights
    let adminbarHeight = 0;
    let menuHeight = 0;

    win.on("load", function () {

        // Execute on load
        checkSticky();

        // Refresh whenever window is resized.
        win.on('resize.sticky', null, null, checkSticky);

    });

    function checkSticky() {

        // if no sticky, get out
        degreeFinder = $('.sidebar .ssr-qdf');
        if (degreeFinder.length !== 1)
            return;

        // only on desktop
        if (win.outerWidth() >= 992) {

            // get the last element in the sidebar, so we know where to stick the df
            let lastInSidebar = $('.sidebar > *').last();
            if (lastInSidebar.length !== 1)
                return;

            // get the last element in the sidebars position from the top
            let lastInSidebarPosition = lastInSidebar.offset().top;

            // get the last element in the sidebars's height
            let lastInSidebarHeight = lastInSidebar.outerHeight();

            // got to have position and height of last element to know where to stick df
            if (!lastInSidebarPosition || !lastInSidebarHeight)
                return;

            // get the admin bar height
            let adminbar = $('#wpadminbar');
            if (adminbar.length === 1)
                adminbarHeight = adminbar.outerHeight();

            // get the admin bar height
            let menu = $('.megamenu');
            if (menu.length === 1)
                menuHeight = menu.outerHeight();

            // so the sticky point is when everything is the bottom of the sidebar
            stickyPoint = lastInSidebarPosition + lastInSidebarHeight - adminbarHeight;

            // get out if content is too short
            let contentBottom, content = $('.content.with-sidebar');
            if (content.length > 0) contentBottom = content.outerHeight() + content.offset().top;
            if (contentBottom < stickyPoint + $(window).height()) return;

            // start with unstuck
            degreeFinder.addClass('unstuck');

            // do it initialy
            doStickiness();

            // do it on scroll
            win.on('scroll.sticky', null, null, doStickiness);

        }

        // make sure to turn it all off
        else {
            win.off('.sticky');
            degreeFinder.removeAttr('style');
        }
    }

    // make it sticky or not
    function doStickiness() {

        // get the current scroll position
        let scrollPosition = win.scrollTop();

        // if we have scrolled past the sticky point and it's not already stuck
        if (scrollPosition > stickyPoint && degreeFinder.hasClass('unstuck')) {

            // get initial width
            let width = degreeFinder.css('width');

            // add the stuck class, remove the unstuck class
            degreeFinder.addClass('stuck').removeClass('unstuck');

            // change the inline styles
            degreeFinder.css({
                'position': 'fixed',
                'top': degreeFinder.offset().top - scrollPosition + 'px',
                'width': width
            }).animate({top: menuHeight + adminbarHeight + 20 + 'px'}, 250);
        }

        // if we've scrolled up past the sticky point, unstick em all
        if (scrollPosition < stickyPoint && degreeFinder.hasClass('stuck')) {

            // add the unstuck class, remove the stuck class
            degreeFinder.removeClass('stuck').addClass('unstuck');

            degreeFinder.animate({top: stickyPoint - scrollPosition + 'px'}, 100, () => {
                degreeFinder.removeAttr('style');
            });
        }

    }

}(jQuery, window));</script></body>
